{
    "name":"Remove Related Section",
    "description":"Removes related videos section on YouTube",
    "version":"1",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": ["http://www.youtube.com/watch*","https://www.youtube.com/watch*"],
            "js": ["removeRelated.js"],
            "run_at": "document_start"
        }
        ]
}